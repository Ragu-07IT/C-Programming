#include <stdio.h>

int main() {
int num,rev=0,rem,og;
    scanf("%d",&num);
    og=num;
    while(num != 0){
        rem=num % 10;
        rev= rev * 10 + num;
        num=num / 10;
        
    }
    if(og == rev){
     printf("Palindrome\n");
    }
    else{
        printf("Not Palindrome\n");
    }
    
    return 0;
}

