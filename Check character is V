//Program to check given character is vowel or consonant.
#include<stdio.h>
int main(){
    char ch;
    printf("Enter character:");
    scanf("%c",&ch);
    if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u' || ch=='A' || ch=='E' || ch=='I' || 
        ch=='O' || ch=='U')
        printf("Character is a vowel");
    else
        printf("Character is a consonant");
    return 0;
}
