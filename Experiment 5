//WAP to perform the sum of all the digits of the given number.
#include<stdio.h>
int main(){
    int num,sum=0,digit;
    printf("Enter Number:");
    scanf("%d",&num);

    while (num != 0){
    digit=num%10;
    sum=sum+digit;
    num=num/10;
    
    }
    printf("Sum of digits=%d\n",sum);
    

    return 0;
}
